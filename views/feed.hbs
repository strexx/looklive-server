<section class="feed-items">
    {{#each items}}
        <div class="feed__item" data-uuid="{{id}}">
            <img src="{{ image.url }}">
            <h2><img class="feed__item__avatar" src="{{ subject.avatar.url }}"> {{ subject.name }}</h2>
            <h1><a href="#/appearance/{{id}}">{{title}}</a></h1>
        </div>
    {{/each}}
</section>